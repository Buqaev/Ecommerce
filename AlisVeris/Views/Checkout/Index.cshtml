@model List<AlisVeris.Models.Checkout>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br />


<h2>Alis Siyahisi</h2>

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered">
            <header>
            <tr>
                <th>Alici</th>
                @*<th>Mehsul</th>*@
                <th>Gonderilecek Seher</th>
                <th>Gonderilecek Olke</th>
                <th>Gonderilecek Adres</th>
                
            </tr>
            </header>

            @foreach (var x in Model)
            {
                <tr>
                    <td>@x.User.Ad @x.User.Soyad</td>
                    <td>@x.CatdirilmaSeher</td>
                    <td>@x.CatdirilmaSeheri</td>
                    <td>@x.CatdirilmaAdres</td>
                    
                    

                  


                    
                </tr>
            }
        </table>
    </div>
</div>


